function calculate(num1, string, num2){
  if ((string === "+" || "-" || "*" || "/" || "%") && typeof num1 === "number" && typeof num2 === "number"){
    var result = eval(num1 + string + num2);
  } else {
    console.log("Incorrect input!");
  }
console.log(result);
}

calculate(1,"+",2);
calculate(9,"-",1);
calculate(3,"*",3);
calculate(8,"/",2);
calculate(2,"%",5);
calculate(4,"hi",3);